{
  "rewrites": [
    {
      "source": "/login",
      "destination": "/pages/login.html"
    },
    {
      "source": "/student",
      "destination": "/pages/student.html"
    },
    {
      "source": "/teacher",
      "destination": "/pages/teacher.html"
    },
    {
      "source": "/admin",
      "destination": "/pages/admin.html"
    }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
